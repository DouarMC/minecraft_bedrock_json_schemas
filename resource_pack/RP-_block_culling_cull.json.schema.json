{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "Ce fichier sert à définir les règles de culling de blocs pour un modèle de bloc. \nType: `Object`",
    "type": "object",
    "required": ["format_version"],
    "properties": {
        "format_version": {
            "description": "La version du Format à utiliser. \nType: `String`",
            "$ref": "../common.schema.json#/definitions/minecraft_version",
            "examples": ["1.20.60", "1.20.70", "1.20.80", "1.21.0", "1.21.10", "1.21.20", "1.21.30", "1.21.40", "1.21.50", "1.21.60"]
        }
    },
    "allOf": [
        {
            "if": {"properties": {"format_version": {"enum": ["1.20.60", "1.20.70", "1.20.80", "1.21.0", "1.21.10", "1.21.20", "1.21.30", "1.21.40", "1.21.50", "1.21.60"]}}},
            "then": {"$ref": "../format_version/1.20.60/RP-_block_culling_cull.json.schema.json"}
        }
    ]
}